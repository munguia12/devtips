<p id="notice"><%= notice %></p>

<h1 class="center">Entradas</h1>
<div class="row center-xs">
    <div class="col-xs-8">
           
    <table>
      <thead>
        <tr>
          <th>Fecha</th>
          <th>Remision</th>
          <th>Cantidad</th>
          <th>Articulo</th>
          <th>Refaccion</th>
          <th>Recibe</th>
          <th>Proveedor</th>
          <th>Contacto</th>
          <th>Autoriza</th>
          <th>Solicta</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @entradas.each do |entrada| %>
          <tr>
            <td><%= entrada.fecha %></td>
            <td><%= entrada.remision %></td>
            <td><%= entrada.cantidad %></td>
            <td><%= entrada.articulo %></td>
            <td><%= entrada.refaccion %></td>
            <td><%= entrada.recibe %></td>
            <td><%= entrada.proveedor %></td>
            <td><%= entrada.contacto %></td>
            <td><%= entrada.autoriza %></td>
            <td><%= entrada.solicta %></td>
            <td><%= link_to 'Show', entrada %></td>
            <td><%= link_to 'Edit', edit_entrada_path(entrada) %></td>
            <td><%= link_to 'Destroy', entrada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>
    <div class="button"> 
        <%= link_to 'New Entrada', new_entrada_path %>
     </div> 
   

    </div>
</div>
